#include<iostream>
using namespace std;

int main( ){
	int n;
	cin>>n;   // n=3
	char ch = 'A';
	for(int row=1; row<=n; row++){
		for(int col=1; col<=n; col++){
		cout<<ch<<" ";
		ch++;
		}
	cout<<endl;
	}	
	return 0;
}


