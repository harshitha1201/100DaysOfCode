using namespace std;

int main( ){
	int n;
	cin>>n;   // n=3
	for(int row=1; row<=n; row++){
		for(int col=1; col<=n; col++){
		char value = 'A'+col-1;
		cout<<value<<" ";
		}
	cout<<endl;
	}	
	return 0;
}

