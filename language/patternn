#include<iostream>
using namespace std;

int main( ){
	int n, count =1;
	cin>>n;   // n=3
	for(int row=1; row<=n; row++){
		for(int col=1; col<=n; col++){
			cout<<count<<" ";
			count++;
		}
		cout<<endl;
	}	
	return 0;
}

-------------------------------------------------------

9 8 7
6 5 4
3 2 1

#include<iostream>
using namespace std;

int main( ){
int n;
cin>>n;   // n=3
int  count =3*n;
for(int row=1; row<=n; row++){
	for(int col=1; col<=n; col++){
	cout<<count<<" ";
	count--;
	}
cout<<endl;
	}	
	return 0;
}

