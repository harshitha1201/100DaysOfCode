#include<iostream>
using namespace std;

int main( ){
int n,count=1;
cin>>n;   // n=4
	for(int row=1; row<=n; row++){
		for(int col=1; col<=row; col++){
		cout<<count<<" ";
		count++;
		}
	cout<<endl;
	}	
	return 0;
}

