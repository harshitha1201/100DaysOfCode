#include<iostream>
using namespace std;

int main( ){
int n;
cin>>n;  //n=4
	for(int row =1;row<=n;row++){
		for(int col=1;col<=row;col++){
		char value='A'+row+col-2;
		cout<<value<<" ";
		}
	cout<<endl;
	}
	return 0;
}

